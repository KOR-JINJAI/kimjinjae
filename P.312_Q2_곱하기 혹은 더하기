#my sol
n=input()
result=int(n[0])
length=len(n)
for i in range(1,length):
    a=result + int(n[i])
    b=result * int(n[i])
    if a>=b:
        result=a
    else:
        result=b

print(result)


#st sol
n=input()
result=int(n[0])

for i in range(1,len(n)):
    num= int(n[i])
    if result<=1 or num<=1:
        result+=num
    else:
        result*=num

print(result)
